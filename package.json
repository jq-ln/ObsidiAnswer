{
  "name": "obsidian-rag-plugin",
  "version": "1.0.0",
  "description": "Intelligent knowledge retrieval and question answering using RAG for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:sync": "npm run build && npm run copy-to-vault",
    "copy-to-vault": "node scripts/copy-to-vault.js",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "rag",
    "ai",
    "knowledge",
    "retrieval",
    "question-answering"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "obsidian": "^1.4.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  }
}
